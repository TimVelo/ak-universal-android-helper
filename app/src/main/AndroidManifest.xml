<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ak.andro.kumaraakash86.akuhsampleapp">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="ak.andro.kumaraakash86.akuhsampleapp.MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ak.andro.kumaraakash86.akuhsampleapp.samples.LogMessagesActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="ak.andro.kumaraakash86.akuhsampleapp.samples.DeviceStatsActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="ak.andro.kumaraakash86.akuhsampleapp.samples.AlertsActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="ak.andro.kumaraakash86.akuhsampleapp.samples.SnackbarsActivity" android:screenOrientation="portrait"></activity>
    </application>

</manifest>